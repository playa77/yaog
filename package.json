  "name": "yaog",
  "version": "7.1.1",
  "description": "Yet another OpenRouter GUI â€” immersive AI chat for solo & group RPG",
  "author": {
    "name": "Dave Martin",
    "email": "playa77@gmail.com"
  },
  "homepage": "https://github.com/playa77/yaog",
  "type": "module",
  "main": "electron/main.cjs",
